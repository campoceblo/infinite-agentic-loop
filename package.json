{
  "name": "infinite-agentic-loop-mistral",
  "version": "1.0.0",
  "description": "Infinite Agentic Loop implementation using MistralAI APIs",
  "main": "src/infinite-loop.js",
  "type": "module",  "scripts": {
    "start": "node src/infinite-loop.js",
